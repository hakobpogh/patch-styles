{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "project": "tsconfig.json",
    "tsconfigRootDir": "./",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "react-hooks"],
  "extends": [
    "eslint:recommended",
    "react-app",
    "plugin:react/recommended"
  ],
  "rules": {
    "accessor-pairs": "off",
    "array-bracket-newline": ["warn", "consistent"],
    "array-bracket-spacing": ["warn", "never"],
    "array-callback-return": [
      "warn",
      {
        "allowImplicit": false,
        "checkForEach": false
      }
    ],
    "array-element-newline": ["warn", "consistent"],
    "arrow-body-style": ["off", "as-needed"],
    "arrow-parens": ["warn", "always"],
    "arrow-spacing": [
      "warn",
      {
        "before": true,
        "after": true
      }
    ],
    "block-scoped-var": "warn",
    "block-spacing": ["warn", "always"],
    "brace-style": ["warn", "1tbs"],
    "camelcase": [
      "off",
      {
        "properties": "always",
        "allow": []
      }
    ],
    "capitalized-comments": ["off", "never"],
    "class-methods-use-this": "warn",
    "comma-dangle": ["warn", "always-multiline"],
    "comma-spacing": ["warn"],
    "comma-style": ["warn", "last"],
    "complexity": ["warn", 5],
    "computed-property-spacing": ["warn", "never"],
    "consistent-return": "off",
    "consistent-this": ["warn", "this"],
    "curly": ["warn", "all"],
    "default-case": "warn",
    "default-case-last": "warn",
    "default-param-last": "warn",
    "dot-notation": "warn",
    "eqeqeq": ["warn", "always"],
    "eol-last": ["warn", "always"],
    "func-call-spacing": ["warn", "never"],
    "func-name-matching": ["warn", "always"],
    "func-names": ["warn", "never"],
    "func-style": [
      "warn",
      "declaration",
      {
        "allowArrowFunctions": true
      }
    ],
    "function-call-argument-newline": ["warn", "consistent"],
    "function-paren-newline": ["warn", "consistent"],
    "generator-star-spacing": [
      "warn",
      {
        "before": false,
        "after": true
      }
    ],
    "grouped-accessor-pairs": ["warn", "getBeforeSet"],
    "guard-for-in": "warn",
    "id-denylist": ["warn", "__proto__", "__any_other_id__"],
    "id-length": [
      "warn",
      {
        "min": 2,
        "max": 42
      }
    ],
    "id-match": "off",
    "implicit-arrow-linebreak": ["warn", "beside"],
    "import/extensions": ["off"],
    "import/no-cycle": ["off"],
    "import/first": "warn",
    "import/no-amd": "warn",
    "import/no-anonymous-default-export": "warn",
    "import/no-unresolved": [
      "warn",
      { "ignore": ["@app/.*"] }
    ],
    "import/no-webpack-loader-syntax": "warn",
    "import/order": [
      "warn",
      {
        "groups": [
          "object",
          "builtin",
          "external",
          ["internal", "parent", "sibling"],
          "index"
        ]
      }
    ],
    "indent": ["warn", 2],
    "init-declarations": ["warn", "always"],
    "jsx-quotes": ["warn", "prefer-double"],
    "key-spacing": ["warn"],
    "keyword-spacing": ["warn"],
    "line-comment-position": [
      "warn",
      {
        "position": "above"
      }
    ],
    "linebreak-style": ["warn", "unix"],
    "lines-around-comment": ["warn"],
    "lines-between-class-members": ["warn", "always"],
    "max-classes-per-file": ["warn", 4],
    "max-depth": ["warn", 3],
    "max-len": [
      "warn",
      128,
      {
        "ignoreUrls": true
      }
    ],
    "max-lines": ["warn", 256],
    "max-lines-per-function": "off",
    "max-nested-callbacks": ["warn", 5],
    "max-params": ["warn", 5],
    "max-statements": "off",
    "max-statements-per-line": [
      "warn",
      {
        "max": 1
      }
    ],
    "multiline-comment-style": "off",
    "multiline-ternary": "off",
    "new-cap": "warn",
    "new-parens": ["warn", "always"],
    "newline-before-return": "off",
    "newline-per-chained-call": [
      "off",
      {
        "ignoreChainWithDepth": 2
      }
    ],
    "no-alert": "warn",
    "no-array-constructor": "warn",
    "no-await-in-loop": "off",
    "no-bitwise": [
      "warn",
      {
        "int32Hint": true
      }
    ],
    "no-caller": "error",
    "no-confusing-arrow": "warn",
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "no-const-assign": "warn",
    "no-constructor-return": "warn",
    "no-continue": "off",
    "no-div-regex": "warn",
    "no-duplicate-imports": "warn",
    "no-else-return": [
      "warn",
      {
        "allowElseIf": false
      }
    ],
    "no-empty-function": "warn",
    "no-empty-pattern": "warn",
    "no-eq-null": "warn",
    "no-eval": "warn",
    "no-extend-native": "warn",
    "no-extra-bind": "warn",
    "no-extra-label": "warn",
    "no-extra-parens": [
      "off",
      "all",
      {
        "ignoreJSX": "all"
      }
    ],
    "no-fallthrough": "warn",
    "no-floating-decimal": "warn",
    "no-global-assign": "warn",
    "no-implicit-coercion": [
      "off",
      {
        "boolean": false
      }
    ],
    "no-implicit-globals": "warn",
    "no-implied-eval": "warn",
    "no-inline-comments": "warn",
    "no-invalid-this": [
      "warn",
      {
        "capIsConstructor": false
      }
    ],
    "no-iterator": "warn",
    "no-labels": "warn",
    "no-label-var": "warn",
    "no-lone-blocks": "warn",
    "no-lonely-if": "warn",
    "no-loop-func": "warn",
    "no-loss-of-precision": "warn",
    "no-magic-numbers": [
      "off",
      {
        "ignoreArrayIndexes": true,
        "ignoreDefaultValues": true,
        "ignore": [0, 1]
      }
    ],
    "no-mixed-operators": "off",
    "no-mixed-spaces-and-tabs": "warn",
    "no-multi-assign": "warn",
    "no-multi-spaces": "warn",
    "no-multi-str": "warn",
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 3,
        "maxEOF": 1,
        "maxBOF": 0
      }
    ],
    "no-negated-condition": "warn",
    "no-nested-ternary": "warn",
    "no-new": "warn",
    "no-new-func": "warn",
    "no-new-object": "warn",
    "no-new-wrappers": "warn",
    "no-octal-escape": "warn",
    "no-param-reassign": [
      "warn",
      {
        "props": false
      }
    ],
    "no-plusplus": "off",
    "no-promise-executor-return": "warn",
    "no-proto": "warn",
    "no-prototype-builtins": "off",
    "no-restricted-exports": ["off"],
    "no-restricted-globals": ["warn", "event"],
    "no-restricted-imports": ["warn", "rxjs/internal/*"],
    "no-restricted-properties": [
      "warn",
      {
        "property": "__defineGetter__",
        "message": "Please use Object.defineProperty instead."
      },
      {
        "property": "__defineSetter__",
        "message": "Please use Object.defineProperty instead."
      }
    ],
    "no-restricted-syntax": [
      "warn",
      "WithStatement",
      "BinaryExpression[operator='in']"
    ],
    "no-return-assign": ["off", "always"],
    "no-return-await": "off",
    "no-script-url": "warn",
    "no-self-compare": "warn",
    "no-sequences": "warn",
    "no-shadow": ["warn"],
    "no-tabs": "warn",
    "no-template-curly-in-string": "warn",
    "no-ternary": "off",
    "no-throw-literal": "warn",
    "no-trailing-spaces": "warn",
    "no-undef-init": "warn",
    "no-underscore-dangle": "warn",
    "no-unmodified-loop-condition": "warn",
    "no-unneeded-ternary": "warn",
    "no-unreachable-loop": "warn",
    "no-unused-expressions": [
      "warn",
      {
        "allowShortCircuit": true,
        "allowTernary": true
      }
    ],
    "no-use-before-define": ["off"],
    "no-useless-backreference": "warn",
    "no-useless-call": "warn",
    "no-useless-computed-key": [
      "warn",
      {
        "enforceForClassMembers": true
      }
    ],
    "no-useless-concat": "warn",
    "no-useless-constructor": "warn",
    "no-useless-rename": "warn",
    "no-useless-return": "warn",
    "no-void": [
      "off",
      {
        "allowAsStatement": true
      }
    ],
    "no-var": "warn",
    "no-warning-comments": "off",
    "no-whitespace-before-property": "warn",
    "object-curly-newline": [
      "warn",
      {
        "consistent": true
      }
    ],
    "object-curly-spacing": ["warn", "always"],
    "object-property-newline": [
      "warn",
      {
        "allowAllPropertiesOnSameLine": true
      }
    ],
    "object-shorthand": "off",
    "one-var": ["warn", "never"],
    "one-var-declaration-per-line": ["warn", "always"],
    "operator-assignment": ["warn", "always"],
    "operator-linebreak": [
      "warn",
      "after",
      {
        "overrides": {
          "?": "before",
          ":": "before"
        }
      }
    ],
    "prefer-arrow-callback": ["warn"],
    "prefer-const": "warn",
    "prefer-destructuring": [
      "warn",
      {
        "object": false,
        "array": true
      },
      {
        "enforceForRenamedProperties": false
      }
    ],
    "prefer-named-capture-group": "warn",
    "prefer-numeric-literals": "warn",
    "prefer-promise-reject-errors": "warn",
    "padded-blocks": ["warn", "never"],
    "padding-line-between-statements": "off",
    "prefer-exponentiation-operator": "warn",
    "prefer-object-spread": "warn",
    "prefer-regex-literals": [
      "warn",
      {
        "disallowRedundantWrapping": true
      }
    ],
    "prefer-rest-params": "warn",
    "prefer-spread": "warn",
    "prefer-template": "warn",
    "quote-props": ["warn", "as-needed"],
    "quotes": ["warn", "single"],
    "radix": ["warn", "as-needed"],
    "react/boolean-prop-naming": [
      "warn",
      {
        "rule": "^(is|has|are)[A-Z]([A-Za-z0-9]?)+",
        "validateNested": true
      }
    ],
    "react/button-has-type": "warn",
    "react/default-props-match-prop-types": "warn",
    "react/destructuring-assignment": "off",
    "react/display-name": "off",
    "react/forbid-component-props": "off",
    "react/forbid-dom-props": "off",
    "react/forbid-elements": [
      "off",
      {
        "forbid": ["table", "thead", "tbody", "tr", "th", "td"]
      }
    ],
    "react/forbid-foreign-prop-types": "warn",
    "react/forbid-prop-types": [
      "warn",
      {
        "forbid": ["any", "array", "object"]
      }
    ],
    "react/function-component-definition": [
      "warn",
      {
        "namedComponents": "arrow-function",
        "unnamedComponents": "arrow-function"
      }
    ],
    "react/jsx-boolean-value": ["warn", "never"],
    "react/jsx-child-element-spacing": "warn",
    "react/jsx-closing-bracket-location": ["warn", "line-aligned"],
    "react/jsx-closing-tag-location": "warn",
    "react/jsx-curly-brace-presence": ["warn", "never"],
    "react/jsx-curly-newline": [
      "warn",
      {
        "multiline": "consistent",
        "singleline": "consistent"
      }
    ],
    "react/jsx-curly-spacing": ["warn"],
    "react/jsx-equals-spacing": ["warn", "never"],
    "react/jsx-filename-extension": [
      "warn",
      {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    ],
    "react/jsx-first-prop-new-line": ["warn", "multiline-multiprop"],
    "react/jsx-fragments": ["warn", "element"],
    "react/jsx-handler-names": [
      "warn",
      {
        "eventHandlerPrefix": false,
        "eventHandlerPropPrefix": "off",
        "checkLocalVariables": true,
        "checkInlineFunction": false
      }
    ],
    "react/jsx-indent-props": ["warn", "first"],
    "react/jsx-indent": [
      "warn",
      2,
      {
        "indentLogicalExpressions": true
      }
    ],
    "react/jsx-key": [
      "warn",
      {
        "checkFragmentShorthand": true
      }
    ],
    "react/jsx-max-depth": [
      "warn",
      {
        "max": 5
      }
    ],
    "react/jsx-max-props-per-line": [
      "warn",
      {
        "maximum": 1,
        "when": "multiline"
      }
    ],
    "react/jsx-no-bind": [
      "warn",
      {
        "allowArrowFunctions": true
      }
    ],
    "react/jsx-no-comment-textnodes": "warn",
    "react/jsx-no-duplicate-props": ["warn"],
    "react/jsx-no-literals": "off",
    "react/jsx-no-script-url": "warn",
    "react/jsx-no-target-blank": [
      "warn",
      {
        "enforceDynamicLinks": "always"
      }
    ],
    "react/jsx-no-undef": ["warn"],
    "react/jsx-no-useless-fragment": "warn",
    "react/jsx-one-expression-per-line": "off",
    "react/jsx-pascal-case": ["warn"],
    "react/jsx-props-no-multi-spaces": "warn",
    "react/jsx-props-no-spreading": [
      "off",
      {
        "exceptions": []
      }
    ],
    "react/jsx-sort-default-props": "off",
    "react/jsx-sort-props": [
      "warn",
      {
        "callbacksLast": true,
        "shorthandFirst": false,
        "shorthandLast": false,
        "ignoreCase": false,
        "noSortAlphabetically": true,
        "reservedFirst": true
      }
    ],
    "react/jsx-tag-spacing": ["warn"],
    "react/jsx-uses-vars": "warn",
    "react/jsx-uses-react": "warn",
    "react/jsx-wrap-multilines": [
      "warn",
      {
        "declaration": "parens-new-line",
        "assignment": "parens-new-line",
        "return": "parens-new-line",
        "arrow": "parens-new-line",
        "condition": "ignore",
        "logical": "ignore",
        "prop": "ignore"
      }
    ],
    "react/no-access-state-in-setstate": "warn",
    "react/no-adjacent-inline-elements": "warn",
    "react/no-array-index-key": "off",
    "react/no-children-prop": "warn",
    "react/no-danger-with-children": "warn",
    "react/no-danger": "warn",
    "react/no-deprecated": "warn",
    "react/no-did-mount-set-state": "warn",
    "react/no-did-update-set-state": "warn",
    "react/no-direct-mutation-state": "warn",
    "react/no-find-dom-node": "warn",
    "react/no-is-mounted": "warn",
    "react/no-multi-comp": [
      "warn",
      {
        "ignoreStateless": true
      }
    ],
    "react/no-redundant-should-component-update": "warn",
    "react/no-render-return-value": "warn",
    "react/no-set-state": "off",
    "react/no-string-refs": "warn",
    "react/no-this-in-sfc": "warn",
    "react/no-typos": "warn",
    "react/no-unescaped-entities": ["warn"],
    "react/no-unknown-property": ["warn"],
    "react/no-unsafe": [
      "warn",
      {
        "checkAliases": true
      }
    ],
    "react/no-unused-prop-types": ["warn"],
    "react/no-unused-state": "warn",
    "react/no-will-update-set-state": "warn",
    "react/prefer-es6-class": ["warn", "always"],
    "react/prefer-read-only-props": "warn",
    "react/prefer-stateless-function": "warn",
    "react/prop-types": [
      "warn",
      {
        "skipUndeclared": true
      }
    ],
    "react/react-in-jsx-scope": "warn",
    "react/require-default-props": [
      "off",
      {
        "ignoreFunctionalComponents": true
      }
    ],
    "react/require-optimization": ["off"],
    "react/require-render-return": "warn",
    "react/self-closing-comp": ["warn"],
    "react/sort-comp": [
      "warn",
      {
        "order": [
          "static-variables",
          "instance-variables",
          "getters",
          "setters",
          "constructor",
          "lifecycle",
          "render",
          "instance-methods",
          "everything-else",
          "static-methods"
        ]
      }
    ],
    "react/sort-prop-types": ["off"],
    "react/state-in-constructor": ["warn", "never"],
    "react/static-property-placement": ["off"],
    "react/style-prop-object": ["warn"],
    "react/void-dom-elements-no-children": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "react-hooks/rules-of-hooks": "warn",
    "require-atomic-updates": "warn",
    "require-await": "warn",
    "require-unicode-regexp": "off",
    "rest-spread-spacing": ["warn", "never"],
    "semi": ["warn", "always"],
    "semi-spacing": [
      "warn",
      {
        "before": false,
        "after": true
      }
    ],
    "semi-style": ["warn", "last"],
    // using import/order
    "sort-imports": ["off"],
    "sort-keys": "off",
    "sort-vars": "warn",
    "symbol-description": "warn",
    "space-before-blocks": ["warn", "always"],
    "space-before-function-paren": [
      "warn",
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "space-in-parens": ["warn", "never"],
    "space-infix-ops": "warn",
    "space-unary-ops": "warn",
    "spaced-comment": [
      "warn",
      "always",
      {
        "block": {
          "balanced": true
        },
        "markers": ["/"]
      }
    ],
    "strict": ["warn", "never"],
    "switch-colon-spacing": "warn",
    "template-curly-spacing": ["warn", "never"],
    "template-tag-spacing": ["warn", "never"],
    "unicode-bom": ["warn", "never"],
    "vars-on-top": "off",
    "wrap-iife": [
      "warn",
      "inside",
      {
        "functionPrototypeMethods": true
      }
    ],
    "wrap-regex": "off",
    "yoda": [
      "warn",
      "never",
      {
        "exceptRange": true
      }
    ],
    "yield-star-spacing": ["warn", "after"]
  },
  "reportUnusedDisableDirectives": true,
  "overrides": [
    {
      "files": ["**/*.test.ts", "**/*.test.tsx"],
      "extends": [
        "plugin:jest/recommended",
        "plugin:jest/style",
        "react-app/jest"
      ],
      "plugins": ["jest"],
      "rules": {
        "import/no-extraneous-dependencies": [
          "warn",
          {
            "devDependencies": true
          }
        ]
      }
    },
    {
      "files": ["**/*.ts", "**/*.tsx"],
      "parser": "@typescript-eslint/parser",
      "extends": [
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:import/typescript"
      ],
      "rules": {
        // enabling typescript version for enums, interfaces, etc
        "brace-style": "off",
        "comma-dangle": "off",
        "comma-spacing": "off",
        "default-param-last": "off",
        "dot-notation": "off",
        "func-call-spacing": "off",
        "indent": "off",
        "init-declarations": "off",
        "keyword-spacing": "off",
        "lines-between-class-members": "off",
        "no-array-constructor": "off",
        "no-dupe-class-members": "off",
        "no-duplicate-imports": "off",
        "no-empty-function": "off",
        "no-extra-parens": "off",
        "no-extra-semi": "off",
        "no-implied-eval": "off",
        "no-invalid-this": "off",
        "no-loop-func": "off",
        "no-magic-numbers": "off",
        "no-redeclare": "off",
        "no-shadow": "off",
        "no-throw-literal": "off",
        "no-unused-expressions": "off",
        "no-unused-vars": "off",
        "no-use-before-define": "off",
        "no-useless-constructor": "off",
        "quotes": "off",
        "require-await": "off",
        "no-return-await": "off",
        "semi": "off",
        "space-before-function-paren": "off",
        "space-infix-ops": "off",
        "@typescript-eslint/array-type": ["warn"],
        "@typescript-eslint/ban-tslint-comment": "warn",
        "@typescript-eslint/ban-types": ["warn"],
        "@typescript-eslint/brace-style": "warn",
        "@typescript-eslint/class-literal-property-style": ["warn", "fields"],
        "@typescript-eslint/comma-dangle": ["warn", "always-multiline"],
        "@typescript-eslint/comma-spacing": ["warn"],
        "@typescript-eslint/consistent-indexed-object-style": [
          "warn",
          "record"
        ],
        "@typescript-eslint/consistent-type-assertions": ["off"],
        "@typescript-eslint/consistent-type-definitions": [
          "warn",
          "interface"
        ],
        "@typescript-eslint/consistent-type-imports": [
          "warn",
          {
            "prefer": "no-type-imports",
            "disallowTypeAnnotations": true
          }
        ],
        "@typescript-eslint/default-param-last": "warn",
        "@typescript-eslint/dot-notation": ["warn"],
        "@typescript-eslint/explicit-function-return-type": [
          "off",
          {
            "allowExpressions": true,
            "allowHigherOrderFunctions": true,
            "allowTypedFunctionExpressions": true,
            "allowDirectConstAssertionInArrowFunctions": true,
            "allowConciseArrowFunctionExpressionsStartingWithVoid": true
          }
        ],
        "@typescript-eslint/explicit-member-accessibility": [
          "warn",
          {
            "accessibility": "no-public"
          }
        ],
        "@typescript-eslint/explicit-module-boundary-types": ["off"],
        "@typescript-eslint/func-call-spacing": ["warn", "never"],
        "@typescript-eslint/indent": ["warn", 2],
        "@typescript-eslint/init-declarations": ["warn", "always"],
        "@typescript-eslint/keyword-spacing": ["warn"],
        "@typescript-eslint/lines-between-class-members": ["warn", "always"],
        "@typescript-eslint/member-delimiter-style": [
          "warn",
          {
            "multiline": {
              "delimiter": "semi",
              "requireLast": true
            },
            "singleline": {
              "delimiter": "semi",
              "requireLast": true
            }
          }
        ],
        "@typescript-eslint/member-ordering": ["warn"],
